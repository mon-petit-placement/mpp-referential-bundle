{
    "name": "mpp/referential-bundle",
    "type": "symfony-bundle",
    "description": "Symfony referential Bundle",
    "keywords": ["referential", "json", "yaml", "api"],
    "license": "MIT",
    "authors": [
      {
        "name": "Gabriel BONDAZ",
        "email": "gabriel.bondaz@idci-consulting.fr",
        "homepage": "https://www.idci-consulting.fr"
      }
    ],
    "require": {
      "php": ">=7.4",
      "symfony/config": "^4.0",
      "symfony/dependency-injection": "^4.0",
      "symfony/framework-bundle": "^4.0",
      "symfony/http-foundation": "^4.0",
      "symfony/http-kernel": "^4.0",
      "symfony/mime": "^4.0",
      "symfony/routing": "^4.0",
      "symfony/serializer": "^4.0",
      "ext-json": "*"
    },
    "autoload":{
      "psr-4":{
        "Mpp\\ReferentialBundle\\": ""
      },
      "exclude-from-classmap": [
        "Resources/",
        "Tests/"
      ]
    },
    "require-dev": {
      "phpunit/phpunit": "7"
    }
  }